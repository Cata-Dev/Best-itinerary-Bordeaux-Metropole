{
  "name": "server",
  "description": "",
  "version": "1.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Catatomik",
    "email": "cata@catadev.org"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 18.19.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "pnpm",
    "database": "mongodb",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write --config ../.prettierrc",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "pnpm run mocha",
    "type-check": "tsc -noEmit",
    "lint": "eslint ./src/",
    "full-lint": "npm run lint && npm run type-check",
    "bundle:client": "pnpm run compile && pnpm pack --pack-destination ./public"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.30",
    "@feathersjs/authentication": "^5.0.30",
    "@feathersjs/authentication-client": "^5.0.30",
    "@feathersjs/configuration": "^5.0.30",
    "@feathersjs/errors": "^5.0.30",
    "@feathersjs/feathers": "^5.0.30",
    "@feathersjs/koa": "^5.0.30",
    "@feathersjs/mongodb": "^5.0.30",
    "@feathersjs/schema": "^5.0.30",
    "@feathersjs/socketio": "^5.0.30",
    "@feathersjs/transport-commons": "^5.0.30",
    "@feathersjs/typebox": "^5.0.30",
    "@typegoose/typegoose": "^12.8.0",
    "axios": "^1.7.7",
    "common": "link:../Common",
    "core-js": "^3.38.1",
    "data": "file:../Data",
    "feathers-hooks-common": "^8.2.1",
    "mongoose": "^8.7.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@feathersjs/cli": "5.0.30",
    "@feathersjs/rest-client": "^5.0.30",
    "@types/mocha": "^10.0.9",
    "@types/node": "^18.19.54",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.1"
  }
}
