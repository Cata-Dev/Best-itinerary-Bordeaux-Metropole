{
  "name": "server",
  "description": "",
  "version": "1.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Catatomik",
    "email": "cata@catadev.org"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 18.13.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "npm",
    "database": "mongodb",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "directories": {
      "lib": "src",
      "test": "test"
    },
    "schema": "typebox"
  },
  "main": "lib/client.mjs",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "npm run compile && node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "npm run mocha",
    "lint": "eslint ./src/ --ext .ts"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "dependencies": {
    "@abraham/reflection": "^0.12.0",
    "@feathersjs/adapter-commons": "^5.0.0-pre.38",
    "@feathersjs/authentication": "^5.0.0-pre.38",
    "@feathersjs/authentication-client": "^5.0.0-pre.38",
    "@feathersjs/configuration": "^5.0.0-pre.38",
    "@feathersjs/errors": "^5.0.0-pre.38",
    "@feathersjs/feathers": "^5.0.0-pre.38",
    "@feathersjs/koa": "^5.0.0-pre.38",
    "@feathersjs/mongodb": "^5.0.0-pre.38",
    "@feathersjs/schema": "^5.0.0-pre.38",
    "@feathersjs/socketio": "^5.0.0-pre.38",
    "@feathersjs/transport-commons": "^5.0.0-pre.38",
    "@feathersjs/typebox": "^5.0.0-pre.38",
    "@typegoose/typegoose": "^10.2.0-beta.3",
    "axios": "^1.3.4",
    "feathers-hooks-common": "^7.0.0-pre.1",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "5.0.0-pre.35",
    "@feathersjs/rest-client": "^5.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
