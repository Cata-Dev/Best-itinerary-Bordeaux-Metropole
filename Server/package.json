{
  "name": "server",
  "description": "",
  "version": "1.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Catatomik",
    "email": "cata@catadev.org"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "engines": {
    "node": ">= 17.3.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "npm",
    "database": "mongodb",
    "framework": "express",
    "transports": [
      "rest",
      "websockets"
    ]
  },
  "main": "lib/client.mjs",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "npm run compile && node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "npm run mocha",
    "lint": "eslint ./src/ --ext .ts"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "dependencies": {
    "@feathersjs/configuration": "^5.0.0-pre.32",
    "@feathersjs/errors": "^5.0.0-pre.32",
    "@feathersjs/express": "^5.0.0-pre.32",
    "@feathersjs/feathers": "^5.0.0-pre.32",
    "@feathersjs/schema": "^5.0.0-pre.32",
    "@feathersjs/socketio": "^5.0.0-pre.32",
    "@feathersjs/transport-commons": "^5.0.0-pre.32",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-hooks-common": "^6.1.5",
    "mongoose": "^6.7.2",
    "node-fetch": "^3.3.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.44.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}