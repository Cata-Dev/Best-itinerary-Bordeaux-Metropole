{
  "name": "server",
  "description": "",
  "version": "1.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Catatomik",
    "email": "cata@catadev.org"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 18.19.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "pnpm",
    "database": "mongodb",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write --config ../.prettierrc",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "pnpm run mocha",
    "lint": "eslint ./src/",
    "bundle:client": "pnpm run compile && pnpm pack --pack-destination ./public"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.22",
    "@feathersjs/authentication": "^5.0.22",
    "@feathersjs/authentication-client": "^5.0.22",
    "@feathersjs/configuration": "^5.0.22",
    "@feathersjs/errors": "^5.0.22",
    "@feathersjs/feathers": "^5.0.22",
    "@feathersjs/koa": "^5.0.22",
    "@feathersjs/mongodb": "^5.0.22",
    "@feathersjs/schema": "^5.0.22",
    "@feathersjs/socketio": "^5.0.22",
    "@feathersjs/transport-commons": "^5.0.22",
    "@feathersjs/typebox": "^5.0.22",
    "@typegoose/typegoose": "^12.2.0",
    "axios": "^1.6.7",
    "core-js": "^3.36.0",
    "data": "file:../Data",
    "feathers-hooks-common": "^8.1.2",
    "mongoose": "^8.2.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@feathersjs/cli": "5.0.21",
    "@feathersjs/rest-client": "^5.0.22",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "mocha": "^10.3.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.2"
  }
}
